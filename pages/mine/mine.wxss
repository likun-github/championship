
.personalInfo {
  position: absolute;
  top:50rpx;
  left:50rpx;
  width:650rpx;
  border-radius: 25rpx;
  background-color: white;
  display: flex;
  flex-direction: row;
  box-shadow: 1px 1px 10px rgb(136, 136, 136); 
  height:260rpx;
}
.personalInfo image {
  padding:50rpx;
  display: block;
  width:160rpx;
}
.personalInfo .info {
  display: flex;
  flex-direction: column;
}
.info .name {
  margin-top:50rpx;
  color:rgb(44, 44, 44);
  font-size: 50rpx;
  font-weight: 900;
}
.info .other {
  color:rgb(104,104,104);
  font-size:25rpx;
}
